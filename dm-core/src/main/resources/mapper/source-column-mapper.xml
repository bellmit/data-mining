<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jdrx.dm.core.mapper.SourceColumnMapper">
    <insert id="save">
        INSERT INTO SOURCE_COLUMN(
        source_id,
        old_name,
        new_name,
        old_idx,
        new_idx,
        col_type
        )VALUES(
        #{sourceId},
        #{oldName},
        #{newName},
        #{oldIdx},
        #{newIdx},
        #{colType}
        )
    </insert>
    <update id="update"></update>
    <update id="executeUpdate"></update>
    <select id="query"></select>
    <select id="queryPager"></select>
    <select id="find"></select>
    <delete id="delete"></delete>
</mapper>